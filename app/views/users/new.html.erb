<h1>New User Sign Up</h1>

<%= render partial: "layouts/error_messages", locals: {object: @user} %>
<%= form_for(@user, url:'/signup') do |f|%>

    <%= f.label :username %>
    <%= f.text_field :username %>
    <br>
    <br>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <br>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    <br>
    <%= f.submit "Submit" %>
   
    <% end %>

